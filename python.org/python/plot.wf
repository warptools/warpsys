{
	"inputs": {
		"rootfs": "catalog:warpsys.org/bootstrap-rootfs:bullseye-1646092800:amd64"
	},
	"steps": {
		"build": {
			"protoformula": {
				"inputs": {
					"/": "pipe::rootfs",
					"/src": "catalog:python.org/python:v3.10.2:src"
				},
				"action": {
					"script": {
						"interpreter": "/bin/sh",
						"contents": [
							"cd /src/*",
							"mkdir -v build",
							"cd build",
							"export SOURCE_DATE_EPOCH=1262304000",
							"../configure --prefix=/prefix/"
							"make",
							"make DESTDIR=/out install",
							"find /out -type d -name __pycache__ -exec rm -rf {} +"
						]
					}
				},
				"outputs": {
					"out": {
						"from": "/out/prefix",
						"packtype": "tar"
					}
				}
			}
		}
	},
	"outputs": {
		"amd64": "pipe:build:out"
	}
}
