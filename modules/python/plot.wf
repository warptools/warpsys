						"/pkg/warpsys.org/findutils": "catalog:warpsys.org/findutils:v4.9.0-2:amd64",
						"/pkg/warpsys.org/zlib": "catalog:warpsys.org/zlib:v1.3:amd64"
					},
					"outputs": {
						"out": {
							"from": "/pack",
							"packtype": "tar"
						}
					}
				}
			}
		}
