						"$PATH": "literal:/pkg/warpsys.org/bootstrap/sed/bin:/pkg/warpsys.org/bootstrap/busybox/bin:/pkg/warpsys.org/bootstrap/glibc/bin:/pkg/warpsys.org/bootstrap/ldshim/bin:/pkg/warpsys.org/zlib/bin:/pkg/warpsys.org/findutils/bin:",
