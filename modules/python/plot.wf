			"pack": {
				"protoformula": {
					"action": {
						"script": {
							"contents": [
								"mkdir -vp /pack/lib",
								"mkdir -vp /pack/dynbin",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/ld-linux-x86-64.so.2 /pack/lib",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/libc.so.6 /pack/lib",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/libdl.so.2 /pack/lib",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/libm.so.6 /pack/lib",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/libpthread.so.0 /pack/lib",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/libutil.so.1 /pack/lib",
								"cp /pkg/warpsys.org/bootstrap/glibc/lib/libcrypt.so.1 /pack/lib",
								"cp /pkg/warpsys.org/zlib/lib/libz.so.1 /pack/lib",
								"mv /pack/bin/python3 /pack/dynbin",
								"cp /pkg/warpsys.org/bootstrap/ldshim/ldshim /pack/bin/python3",
								"mv /pack/bin/python3.10 /pack/dynbin",
								"cp /pkg/warpsys.org/bootstrap/ldshim/ldshim /pack/bin/python3.10",
								"find /pack -type d -name __pycache__ -exec rm -rf {} +",
								"sed -i '0,/XORIGIN/{s/XORIGIN/$ORIGIN/}' /pack/dynbin/*"
							],
							"interpreter": "/pkg/warpsys.org/bootstrap/busybox/bin/sh"
